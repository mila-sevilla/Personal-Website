n    // background-attachment: scroll;\n    line-height: 1.5;\n}\n\n// section {\n//     width: 100vw;\n//     height: 100vh;\n// }\n\nh1, h2, h3 {\n    font-family: 'Satisfy', cursive;\n    color: $color-text-light;\n}\n\nh2 {\n    font-size: 80px;\n    padding: 0;\n    text-align: center;\n    margin-bottom: 20px;\n}\n\np {\n    color: $color-text-light;\n    font-size: $font-size-regular;\n}\n\nbutton, \na {\n    &:hover {\n    cursor: pointer;\n    }\n\n    &:focus {\n        outline: none !important;\n        transform: scale(1.1);\n    }\n}\n\n/*////////////////////////// Welcome/ Intro Section ///////////////////////////////////////////////*/\n \n.content {\n    height: 100vh;\n}\n\n.logo {\n    background: url(\"assets/logo2.svg\") center no-repeat;\n    background-size: 51px auto;\n    height: 70px;\n    width: 100px;\n    position: fixed;\n    z-index: 99;\n\n    > button {\n        display: block;\n        height: 70px;\n        width: 100px;\n        border: none;\n        background-color: transparent;\n    }\n}\n\n.entry_animation.show {\n    animation: HeroAnimation;\n    animation-duration: 3s;\n    animation-delay: 0.1s;\n    animation-fill-mode: forwards;\n    animation-timing-function: cubic-bezier(0.02, 0.8, 0.2, 1);\n}\n\n.introContent {\n    display: flex;\n    justify-content: center;\n    flex-wrap: wrap;\n\n    h1 {\n        font-size: 105px;\n        width: 100%;\n        height: 125px;\n        text-align: center;\n        margin: 50px 20px 0px 20px;\n        opacity: 0.01;\n    }\n\n    &_portrait {\n        background: url(\"assets/portfolio_pic_v2.png\") center top no-repeat;\n        background-size: 156px;\n        width: 100%;\n        height: 156px;\n        margin: 20px 0px;\n        opacity: 0.01;\n    }\n\n    &_text {\n        margin: 0px 38px;\n        opacity: 0.01;\n    }\n}\n\nnav {\n    display: flex;\n    flex-wrap: wrap;\n    justify-content: center;\n    flex-basis: 200px;\n\n    svg {\n        position: relative;\n        top: -8px;\n        left: -2px;\n        z-index: -1;\n    }\n\n    > button {\n        width: 170px;\n        height: 47px;\n        border: none;\n        background-color: Transparent;\n        padding: 20px 40px 7px 40px;\n        font-family: 'Raleway', sans-serif;\n        font-size: 18px;\n        color: $color-text-light;\n        text-decoration: none;\n        text-align: center;\n        margin: 10px 1.5em;\n    }\n}\n\n.section0Btn:hover {\n    cursor: pointer;\n}\n\n/*////////////////////////// About Me Section ///////////////////////////////////////////////*/\n\n.aboutMeContent {\n    margin: 20px 20px 0px 20px;\n\n    h2 {\n        background: url(\"assets/barcelona.svg\") left center no-repeat;\n        background-size: 94px;\n        width: 280px;\n    }\n\n    &_heading {\n        display: flex;\n        width: 100%;\n        justify-content: center;\n\n    }\n\n    .date {\n        font-weight: 700;\n    }\n}\n\n.timeline {\n    position: relative;\n    margin: 0px 20px;\n\n    .aboutMeCV_descr:last-child {\n        margin-bottom: 0px;\n    }\n}\n\n.line {\n    content: '';\n    position: absolute;\n    width: 3px;\n    background-color: white;\n    left: -20px;\n    opacity: 0.5;\n    top: 6px;\n    bottom: 6px;\n}\n\n.date::after {\n    content: '';\n    position: absolute;\n    border: 5px solid white; \n    border-radius: 50%;\n    opacity: 0.9;\n    left: -23px;\n    margin-top: 5px;\n}\n\n\n\n@keyframes HeroAnimation {\n    0% {\n      opacity: 0;\n      transform: translateY(20px);\n    }\n  \n    100% {\n      opacity: 1;\n      transform: translateY(0);\n    }\n}\n\n/* //////////////////////////////// Contact Section /////////////////////////////////////////// */\n\n.contactContent {\n    display: flex;\n    align-content: center;\n    justify-content: center;\n    flex-flow: row wrap;\n    width: 100vw;\n    height: 100vh;\n    color: white;\n    grid-auto-rows: minmax(60px, auto);\n}\n\n.contactContent > h2 {\n    width: 100%;\n    text-align: center;\n}\n\n.contactContent > .socialIcons {\n    display: flex;\n    align-items: center;\n    justify-content: center;\n    justify-items: center;\n    width: 100%;\n    text-align: center;\n    height: 120px;\n    margin: 10px 0 0 0;\n}\n\n.linkedIn,\n.github,\n.instagram {\n    text-decoration: none;\n    font-size: 18px;\n    color: white;\n    text-align: center;\n    padding: 75px 0 20px 0;\n    margin: 0px 30px;\n    background-position: center top;\n    transition: all .2s ease-in-out;\n}\n\n.linkedIn:hover,\n.github:hover,\n.instagram:hover,\n.email:hover {\n    transform: scale(1.1);\n}\n\n.linkedIn {\n    background: url(\"assets/linkedin.png\") center top no-repeat;\n}\n\n.github {\n    background: url(\"assets/github.png\") center top no-repeat;\n}\n\n.instagram {\n    background: url(\"assets/instagram.png\") center top no-repeat;\n}\n\n.email {\n    color: #D26F28;\n    font-size: 20px;\n    padding: 10px;\n    transition: all .2s ease-in-out;\n}\n\n/* //////////////////////////////// Media Queries /////////////////////////////////////////// */\n\n// @media ( min-width: 750px ) {  \n//     /* .introContent {\n//         top: 60%;\n//     } */\n\n//     nav { \n//         flex-basis: 800px; \n//         position: fixed;\n//         bottom: 50px;\n//     }\n\n//     h1 {\n//         font-size: 36px;\n//     }\n\n//     .content {\n//         overflow: hidden;\n//     }\n\n//     .portrait {\n//         background-size: 60%;\n//         height: 100%;\n//         width: 100%;\n//         grid-column: 8/ span 10;\n//         grid-row: 3/ span 5;\n//         border-radius: 3px;\n//     }\n\n//     .aboutMeContent {\n//         display: grid;\n//         grid-template-columns: repeat(10, 1fr);\n//         grid-template-rows: repeat(10, 1fr);\n//         margin: 0;\n//         align-items: start;\n//         justify-items: start;\n//         height: 100%;\n//         grid-row-gap: 1em;\n//     }\n\n//     .aboutMeContent > div > p {\n//         